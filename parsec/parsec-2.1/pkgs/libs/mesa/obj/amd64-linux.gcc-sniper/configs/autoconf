	$(EXTRA_LIB_PATH) 
GLW_LIB_DEPS = -L$(TOP)/$(LIB_DIR)  \
	$(EXTRA_LIB_PATH) 
APP_LIB_DEPS = $(EXTRA_LIB_PATH)  -lX11 -lXext  -lm -lpthread  -lstdc++ -lXt -lX11 -lXmu -lXt -lX11 -lXi -lm 

# DRI dependencies
DRI_LIB_DEPS = $(EXTRA_LIB_PATH) 
